  "devDependencies": {
    "eslint": "8.10.0",
    "eslint-config-next": "12.1.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-sort-imports-es6-autofix": "^0.6.0",
    "grommet": "^2.21.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.5"
